<Window x:Class="Sistema_Servicio_Social.Interfaz"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sistema_Servicio_Social"
        mc:Ignorable="d"
        Title="Interfaz" Height="300" Width="300">
    <Grid Width="Auto" HorizontalAlignment="Stretch" Margin="5,5,5,5">
        <StackPanel  Width="Auto" Margin="0">
            <Separator Margin="20">                
            </Separator>
            <Button Name="btn_Regresar" Click="btnRegresar" Content="Regresar" Background="#545d6a" Foreground="White" FontSize="20"/>
            <TextBox Name="txtBox" Text=""/>
            <Button Name="btn_Abrir" Click="btnAbrir" Content="Abrir csv" Background="#545d6a" Foreground="White" FontSize="20"/>
            <Image x:Name="image" Height="100" Source="Images/csv.png"/>


        </StackPanel>
    </Grid>

</Window>
