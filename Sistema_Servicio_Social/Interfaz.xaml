<Window x:Class="Sistema_Servicio_Social.Interfaz"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sistema_Servicio_Social"
        mc:Ignorable="d"
        Title="Interfaz" Height="481" Width="420">
    <Grid Width="Auto" HorizontalAlignment="Stretch" Margin="5,5,5,5">
        <GroupBox VerticalAlignment="Top">
            <StackPanel>
                <Button Name="btn_Regresar" Click="btnRegresar" Content="Regresar" Background="#545d6a" Foreground="White" FontSize="20"/>
                <TextBox Name="txtBox" Text=""/>
                <Button Name="btn_Abrir" Click="btnAbrir" Content="Abrir csv" Background="#545d6a" Foreground="White" FontSize="20"/>
                <Button Name="btn_Cargar" Click="btnCargar" Content="Cargar Datos" Background="#545d6a" Foreground="White" FontSize="20"/>
                <Button Name="btn_CartaPresentacion" Click="btnCartaPresentacion" Content="Visualizar Carta presentación" Background="#545d6a" Foreground="White" FontSize="20"/>
            </StackPanel>

        </GroupBox>
    </Grid>

</Window>
